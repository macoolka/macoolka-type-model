    "macoolka-io": "^0.2.0"
  },
  "devDependencies": {},
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(./src/*|\\.(test|spec))\\.(ts|tsx)$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/macoolka/macoolka-fp.git"
  },
  "keywords": [
    "macoolka",
